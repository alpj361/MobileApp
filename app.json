{
  "expo": {
    "name": "vizta",
    "slug": "vizta",
    "scheme": "vibecode",
    "version": "0.0011",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibecodeapp.app",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Vizta uses your calendar to show and manage the events you create.",
        "NSRemindersUsageDescription": "Vizta uses reminders to notify you about upcoming events you save.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.vibecodeapp.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-asset",
      "expo-build-properties",
      "expo-mail-composer",
      "expo-secure-store",
      "expo-sqlite",
      "expo-video",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "e055c204-0d79-46a3-bbf5-b35d45e0bb17"
      }
    }
  }
}
